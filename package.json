{
  "name": "mts",
  "description": "MTS",
  "homepage": "https://github.com/mts/mts/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mts/mts.git"
  },
  "scripts": {
    "clean-up": "rm -rf node_modules package-lock.json .storybook-static coverage && npm i lerna -S && lerna run clean-up --stream",
    "install-packages": "npm install && lerna run install-packages --stream",
    "install-papillonads-latest": "npm i \"@papillonads/components\"@latest && npm i \"@papillonads/library\"@latest && lerna run install-papillonads-latest --stream",
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies",
    "start": "lerna run serve --stream",
    "heroku-postbuild": "npm-run-all install-packages",
    "bootstrap": "npm-run-all build build-storybook lint:format test report-coverage",
    "build": "lerna run build --stream",
    "build-storybook": "NODE_ENV=develop build-storybook -c .storybook -o .storybook-static",
    "prebuild-storybook": "NODE_ENV=develop npm run test:generate-output",
    "lint:format": "npm-run-all lint format",
    "lint": "npm-run-all lint:js:jsx lint:scss",
    "lint:js:jsx": "npm-run-all lint:js:jsx:client lint:js:http-server",
    "lint:js:jsx:client": "eslint --ext .js --ext .jsx packages/client/src --fix",
    "lint:js:http-server": "eslint --ext .js packages/http-server/src --fix",
    "lint:scss": "stylelint \"packages/client/src/**/**/*.scss\" --syntax=scss --fix",
    "format": "npm-run-all format:js:jsx format:scss",
    "format:js:jsx": "npm-run-all format:js:jsx:client format:js:http-server",
    "format:js:jsx:client": "prettier --write \"packages/client/src/**/**/*.{js,jsx}\"",
    "format:js:http-server": "prettier --write \"packages/http-server/src/**/**/*.js\"",
    "format:scss": "prettier --write \"packages/client/src/**/**/*.scss\"",
    "test": "jest -u --silent --runInBand --coverage",
    "test:tdd": "jest --watch --onlyChanged --silent=false --verbose",
    "test:generate-output": "jest --coverage=false --json --outputFile=.jest-test-results.json || true",
    "report-coverage": "codecov",
    "start-storybook": "NODE_ENV=develop start-storybook -p 3003 -c .storybook",
    "deploy-client": "gh-pages -d packages/client/build",
    "deploy": "npm-run-all build deploy-client",
    "release": "lerna publish"
  },
